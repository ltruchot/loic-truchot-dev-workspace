{
  "name": "loic-truchot-dev-workspace",
  "description": "Workspace containing the personal website of LoÃ¯c TRUCHOT, french developer",
  "license": "PROPRIETARY",
  "version": "1.0.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "start": "pnpm dev",
    "build": "pnpm --filter @loic-truchot-dev-workspace/website build",
    "dev": "pnpm --filter @loic-truchot-dev-workspace/website dev",
    "preview": "pnpm --filter @loic-truchot-dev-workspace/website preview",
    "lint:fix": "eslint --cache --fix --debug "
  },
  "devDependencies": {
    "@eslint/css": "0.6.0",
    "@eslint/js": "9.24.0",
    "@eslint/json": "0.11.0",
    "@eslint/markdown": "6.3.0",
    "@typescript-eslint/parser": "8.29.1",
    "eslint": "9.24.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "typescript-eslint": "8.29.1"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}